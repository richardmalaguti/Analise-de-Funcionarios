    "# 21. Adicione uma nova coluna \"Age Group\" com os valores \"Young\", \"Middle-aged\" ou \"Senior\" com base na Idade.\n",
    "# Add a new column Age Group with values \"Young\", \"Middle-aged\", or \"Senior\" based on Age."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 43,
   "id": "a2b1a184-08c7-453c-aeb9-ca6ceff69003",
   "metadata": {},
   "outputs": [],
   "source": [
    "df['Age Group'] = ''"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 44,
   "id": "dac2b4dd-d335-4aeb-bbe6-81e4c0b07fff",
   "metadata": {},
   "outputs": [],
   "source": [
    "df.loc[(df['Age'] > 18) & (df['Age'] <= 30), 'Age Group'] = 'Young'\n",
    "df.loc[(df['Age'] > 30) & (df['Age'] <= 45), 'Age Group'] = 'Middle-Aged'\n",
    "df.loc[(df['Age'] > 45) & (df['Age'] <= 60), 'Age Group'] = 'High-Age'"
   ]
