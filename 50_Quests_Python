    "# Aplicando uma s√©rie de valores baseados em filtros.\n",
    "\n",
    "df.loc[(df['Experience'] <= 8), 'Experience Level'] = 'Junior'\n",
    "df.loc[(df['Experience'] > 7) & (df['Experience'] <= 15), 'Experience Level'] = 'Mid'\n",
    "df.loc[(df['Experience'] > 15), 'Experience Level'] = 'S√™nior'"
   ]
